})}}/>\r\n      </div>\r\n      <hr></hr>\r\n      <hr></hr>\r\n      <hr></hr>\r\n      <button className=\"joinbtn btnlog\" onClick={this.login.bind(this)}>Submit</button>\r\n    </div>\r\n    </div>\r\n  );\r\n}\r\n}\r\nexport default Login;","C:\\Users\\Kamel\\OneDrive\\Bureau\\workingdata\\RBK immerssiv project Final\\crawlers\\pull after Beliid build db full 25-12\\Happy-campers\\client\\src\\component\\Logo.js",[],"C:\\Users\\Kamel\\OneDrive\\Bureau\\workingdata\\RBK immerssiv project Final\\crawlers\\pull after Beliid build db full 25-12\\Happy-campers\\client\\src\\component\\Landing.js",["93","94","95"],"import React, { Component } from 'react'\r\n//call tools\r\nimport {\r\n    BrowserRouter as Router,\r\n    Switch,\r\n    Route,\r\n    Link\r\n  } from \"react-router-dom\";\r\n//call component\r\nimport Logo from './Logo.js'\r\n\r\nexport default class Landing extends Component {\r\n    render() {\r\n        return (\r\n        <div>\r\n            <nav className=\"navigation\">\r\n                    <Link to=\"/\">\r\n                    <Logo></Logo>\r\n                    </Link>\r\n                <div>\r\n                    <Link className=\"nodec\" to=\"/login\">\r\n                    <h1>Login</h1>\r\n                    </Link>\r\n                    <Link className=\"nodec\" to=\"/signup\">\r\n                    <h1>Signin</h1>\r\n                    </Link>\r\n                </div>\r\n            </nav>\r\n            <div className=\"landing\">\r\n                <Link  className=\"nodec\" to='/signup'>\r\n                <button className=\"btn_joion\">Join US</button>\r\n                </Link>\r\n            </div>\r\n        </div>\r\n        )\r\n    }\r\n}","C:\\Users\\Kamel\\OneDrive\\Bureau\\workingdata\\RBK immerssiv project Final\\crawlers\\pull after Beliid build db full 25-12\\Happy-campers\\client\\src\\component\\feeds.js",["96","97","98"],"import React, { Component } from 'react'\r\n\r\nimport {\r\n    BrowserRouter as Rou